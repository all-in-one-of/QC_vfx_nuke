].setVisible(True)\n\tthisNode\[\"red_size\"].setVisible(True)\n\tthisNode\[\"red_blur\"].setVisible(True)\n\tthisNode\[\"gp4\"].setVisible(True)\nelse:\n\tred = \"\"\n\tthisNode\[\"red_ec\"].setEnabled(False)\n\tthisNode\[\"red_gamma\"].setEnabled(False)\n\tthisNode\[\"red_erode\"].setEnabled(False)\n\tthisNode\[\"red_amount\"].setEnabled(False)\n\tthisNode\[\"red_size\"].setEnabled(False)\n\tthisNode\[\"red_blur\"].setEnabled(False)\n\tthisNode\[\"gp4\"].setEnabled(False)\n\tthisNode\[\"red_ec\"].setVisible(False)\n\tthisNode\[\"red_gamma\"].setVisible(False)\n\tthisNode\[\"red_erode\"].setVisible(False)\n\tthisNode\[\"red_amount\"].setVisible(False)\n\tthisNode\[\"red_size\"].setVisible(False)\n\tthisNode\[\"red_blur\"].setVisible(False)\n\tthisNode\[\"gp4\"].setVisible(False)\n\nif green_Expression == 0:\n\tgreen = \" green\"\n\tthisNode\[\"green_ec\"].setEnabled(True)\n\tthisNode\[\"green_gamma\"].setEnabled(True)\n\tthisNode\[\"green_erode\"].setEnabled(True)\n\tthisNode\[\"green_amount\"].setEnabled(True)\n\tthisNode\[\"green_size\"].setEnabled(True)\n\tthisNode\[\"green_blur\"].setEnabled(True)\n\tthisNode\[\"gp5\"].setEnabled(True)\n\tthisNode\[\"green_ec\"].setVisible(True)\n\tthisNode\[\"green_gamma\"].setVisible(True)\n\tthisNode\[\"green_erode\"].setVisible(True)\n\tthisNode\[\"green_amount\"].setVisible(True)\n\tthisNode\[\"green_size\"].setVisible(True)\n\tthisNode\[\"green_blur\"].setVisible(True)\n\tthisNode\[\"gp5\"].setVisible(True)\nelse:\n\tgreen = \"\"\n\tthisNode\[\"green_ec\"].setEnabled(False)\n\tthisNode\[\"green_gamma\"].setEnabled(False)\n\tthisNode\[\"green_erode\"].setEnabled(False)\n\tthisNode\[\"green_amount\"].setEnabled(False)\n\tthisNode\[\"green_size\"].setEnabled(False)\n\tthisNode\[\"green_blur\"].setEnabled(False)\n\tthisNode\[\"gp5\"].setEnabled(False)\n\tthisNode\[\"green_ec\"].setVisible(False)\n\tthisNode\[\"green_gamma\"].setVisible(False)\n\tthisNode\[\"green_erode\"].setVisible(False)\n\tthisNode\[\"green_amount\"].setVi